@page "/"
@inject HttpClient Http

<PageTitle>Index</PageTitle>

<h1>Hello, Evryone</h1>
Welcome to your new app.

<SurveyPrompt Title="How is Blazor working for you?" />


@code {
    protected async override Task OnInitializedAsync()
    {
        var result = await Http.GetFromJsonAsync<Key>("home/key");
        Console.WriteLine(result.EnvKey);
        Console.WriteLine(result.UserKey);
    }

    record Key(string EnvKey, string UserKey);
}
